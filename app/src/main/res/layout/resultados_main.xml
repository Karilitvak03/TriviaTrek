<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".ResultadosActivity"
                                                   android:background="@drawable/fondo_login">

    <ImageView
            android:id="@+id/imgLogo4"
            android:layout_width="120dp"
            android:layout_height="118dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/logo2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="0.001"/>
    <TextView
            android:id="@+id/textResultados"
            android:text=""
            android:layout_width="327dp"
            android:layout_height="525dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.634"
            android:textColor="@color/rosado_logo"/>

    <Button
            android:id="@+id/btnVolver5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="VOLVER"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="664dp"
            app:layout_constraintHorizontal_bias="0.134"
            android:backgroundTint="@color/azuloscuro_logo"
            android:textColor="@color/celeste_logo" app:layout_constraintVertical_bias="0.157"/>
    <Button
            android:id="@+id/btnCerrarSesion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="CERRAR SESION"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="664dp"
            app:layout_constraintHorizontal_bias="0.836"
            android:backgroundTint="@color/azuloscuro_logo"
            android:textColor="@color/celeste_logo" app:layout_constraintVertical_bias="0.157"/>
</androidx.constraintlayout.widget.ConstraintLayout>